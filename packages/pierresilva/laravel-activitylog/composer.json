{
    "name": "pierresilva/laravel-activitylog",
    "description": "A very simple activity logger to monitor the users of your website or application",
    "homepage": "https://github.com/pierresilva/laravel-activitylog",
    "keywords":
    [
        "log",
        "user",
        "activity",
        "laravel"
    ],
    "authors": [
        {
            "name": "Pierre silva",
            "email": "pierremichelsilva@gmail.com"
        }
    ],
    "require": {
        "php" : "^7.0",
        "illuminate/config": "~5.5.0|~5.6.0",
        "illuminate/database": "~5.5.0|~5.6.0",
        "illuminate/support": "~5.5.0|~5.6.0",
        "pierresilva/laravel-string": "^2.1"

    },
    "require-dev": {
        "phpunit/phpunit": "^6.2|^7.0"
    },
    "autoload": {
        "psr-4": {
            "pierresilva\\Activitylog\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "license": "MIT",
    "extra": {
        "laravel": {
            "providers": [
                "pierresilva\\Activitylog\\ActivitylogServiceProvider"
            ]
        }
    }
}
