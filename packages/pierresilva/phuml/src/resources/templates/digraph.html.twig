{% extends 'themes/' ~ style.theme %}

{% block content %}
digraph "{{ digraph.id }}" {
splines = true;
overlap = false;
mindist = 0.6;
{% for element in digraph.elements %}
{% spaceless %}
    {% set dot %}
        {% include element.dotTemplate ~ '.html.twig'  with {'element': element, 'style': style} %}
    {% endset %}
{{ dot|replace({ '\n': '', '\r': '' })|raw }}
{% endspaceless %}

{% endfor %}
}
{% endblock %}
