{
    "name": "pierresilva/laravel-query-builder",
    "description": "Easily build Eloquent queries from API requests",
    "keywords": [
        "pierresilva",
        "laravel-query-builder"
    ],
    "homepage": "https://github.com/pierresilva/laravel-query-builder",
    "license": "MIT",
    "authors": [
        {
            "name": "Pierre Silva",
            "email": "pierremichelsilva@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/database": "~5.5.0|~5.6.0|~5.7.0",
        "illuminate/http": "~5.5.0|~5.6.0|~5.7.0",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1|^7.0",
        "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0"
    },
    "autoload": {
        "psr-4": {
            "pierresilva\\QueryBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "pierresilva\\QueryBuilder\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "pierresilva\\QueryBuilder\\QueryBuilderServiceProvider"
            ]
        }
    }
}
